WH b2 != 0 ++ b1 >> b0
# b0       b1                 b2               b3
# val input 1 val to	input2 times to	     counter

>,------------------------------------------------ #input #1
>,------------------------------------------------ #input #2
[<[>>+<<<+>-]>>[<<+>>-] #cp b1 > cpb3 && b3 >> b1
<-] mvb2-- + 1

mv b2 + 48
